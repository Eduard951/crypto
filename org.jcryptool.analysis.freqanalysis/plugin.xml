<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name.0"
            icon="platform:/plugin/org.jcryptool.core.util/icons/analysis_icon.gif"
            category="org.jcryptool.analysis"
            class="org.jcryptool.analysis.freqanalysis.views.FreqAnalysisView"
            id="org.jcryptool.analysis.freqanalysis.views.FreqAnalysisView">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.analysis">
      <analysis
            contextHelpId="org.jcryptool.analysis.freqanalysis.freqanalysis"
            name="%analysis.name.0"
            viewId="org.jcryptool.analysis.freqanalysis.views.FreqAnalysisView"
            icon="platform:/plugin/org.jcryptool.core.util/icons/analysis_icon.gif">
      </analysis>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.jcryptool.analysis.freqanalysis.views.FreqAnalysisView">
         <command
               commandId="org.jcryptool.analysis.freqanalysis.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.png"
               style="push"
               tooltip="%help.command.tooltip">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.png">
         <command
               commandId="org.jcryptool.analysis.freqanalysis.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/save_edit.png"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.jcryptool.analysis.freqanalysis.views.FreqAnalysisView">
         <command
               commandId="org.jcryptool.analysis.freqanalysis.restartCommand"
               icon="platform:/plugin/org.jcryptool.core.util/icons/icon_reset.png"
               style="push"
               tooltip="%restart.command.tooltip">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.analysis.freqanalysis.helpCommand"
            name="%help.command.name">
      </command>
      <command
            id="org.jcryptool.analysis.freqanalysis.restartCommand"
            name="%restart.command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.analysis.freqanalysis.handler.HelpHandler"
            commandId="org.jcryptool.analysis.freqanalysis.helpCommand">
      </handler>
      <handler
            class="org.jcryptool.analysis.freqanalysis.handler.RestartHandler"
            commandId="org.jcryptool.analysis.freqanalysis.restartCommand">
      </handler>
   </extension>

</plugin>
