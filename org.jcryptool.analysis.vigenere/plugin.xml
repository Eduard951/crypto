<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name"
            icon="platform:/plugin/org.jcryptool.core.util/icons/analysis_icon.gif"
            category="org.jcryptool.analysis"
            class="org.jcryptool.analysis.vigenere.views.VigenereBreakerView"
            id="org.jcryptool.analysis.vigenere.views.VigenereBreakerView">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.analysis">
      <analysis
            name="%analysis.name"
            contextHelpId="org.jcryptool.analysis.vigenere.vigenerebreaker"
            viewId="org.jcryptool.analysis.vigenere.views.VigenereBreakerView"
            icon="platform:/plugin/org.jcryptool.core.util/icons/analysis_icon.gif">
      </analysis>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.analysis.vigenere.handler.HelpHandler"
            commandId="org.jcryptool.analysis.vigenere.helpCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.jcryptool.analysis.vigenere.views.VigenereBreakerView">
         <command
               commandId="org.jcryptool.analysis.vigenere.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.png"
               style="push"
               tooltip="%help.command.tooltip">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.png">
         <command
               commandId="org.jcryptool.analysis.vigenere.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/save_edit.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.analysis.vigenere.helpCommand"
            name="Show Help">
      </command>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>

</plugin>
