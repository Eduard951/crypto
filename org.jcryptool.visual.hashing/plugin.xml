<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view.name.0"
            icon="platform:/plugin/org.eclipse.ui/icons/full/eview16/defaultview_misc.png"
            category="org.jcryptool.visual"
            class="org.jcryptool.visual.hashing.views.HashingView"
            id="org.jcryptool.visual.hashing.views.HashingView">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/help/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/help/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.jcryptool.core.operations.visuals">
      <Visual
          	contextHelpId="org.jcryptool.visual.hashing.view"
          	name="%Visual.name.0"
            viewId="org.jcryptool.visual.hashing.views.HashingView"
            icon="platform:/plugin/org.eclipse.ui/icons/full/eview16/defaultview_misc.png">
      </Visual>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.visual.hashing.command.Restart"
            name="%restart.command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.jcryptool.visual.hashing.views.HashingView">
         <command
               commandId="org.jcryptool.visual.hashing.command.Restart"
               icon="platform:/plugin/org.jcryptool.core.util/icons/icon_reset.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.visual.hashing.handlers.RestartHandler"
            commandId="org.jcryptool.visual.hashing.command.Restart">
      </handler>
   </extension>

</plugin>
